{
  "name": "plus-IT/laravel-translation-manager",
  "description": "Manage Laravel Translations",
  "keywords": [
    "laravel",
    "translations",
    "translator",
    "with-tenant"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Matrixhive Solutions",
      "email": "contact@matrixhive.com"
    }
  ],
  "require": {
    "php": ">=7",
    "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x",
    "illuminate/translation": "5.5.x|5.6.x|5.7.x|5.8.x",
    "symfony/finder": "~3.0|~4.0",
    "tanmuhittin/laravel-google-translate": "^0.11.0"
  },
  "autoload": {
    "psr-4": {
      "PlusIT\\TranslationManager\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5-dev"
    },
    "laravel": {
      "providers": [
        "PlusIT\\TranslationManager\\ManagerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
